# Environment variables for Aurora Mock Testing Project
# Copy this file to .env and update with your actual values

# AWS Configuration
AWS_REGION=ap-northeast-1
AWS_PROFILE=default

# Project Configuration
PROJECT_NAME=aurora-mock-testing
ENVIRONMENT=dev

# Aurora Configuration
AURORA_SECRET_ARN=arn:aws:secretsmanager:ap-northeast-1:123456789012:secret:aurora-mock-testing/dev/aurora/connection
DATABASE_NAME=testdb
DB_USERNAME=iam_user

# ECS Configuration
API_URL=http://aurora-mock-testing-dev-alb-123456789.ap-northeast-1.elb.amazonaws.com

# Lambda Configuration
LAMBDA_FUNCTION_NAME=aurora-mock-testing-dev-data-processor

# Testing Configuration
MOCK_MODE=true
TEST_DATABASE_HOST=localhost
TEST_DATABASE_PORT=5432