# AWS Profile (Optional)
AWS_PROFILE=default

# StackSet Management Account
STACK_SET_ACCOUNT=123456789012
STACK_SET_REGION=ap-northeast-1
STACK_SET_ENV_NAME=Production

# StackSet IAM Roles (for SELF_MANAGED mode)
STACK_SET_PERMISSION_MODEL=SELF_MANAGED
STACK_SET_CALL_AS=SELF
STACK_SET_ADMIN_ROLE_ARN=arn:aws:iam::123456789012:role/AWSCloudFormationStackSetAdministrationRole
STACK_SET_EXECUTION_ROLE_NAME=AWSCloudFormationStackSetExecutionRole

# Notification Settings
STACK_SET_NOTIFICATION_MODE=centralized
STACK_SET_NOTIFY_EMAIL=security-ops@example.com
STACK_SET_SLACK_WORKSPACE_ID=TXXXXXXXXXX
STACK_SET_SLACK_CHANNEL_ID=CXXXXXXXXXX

# S3 Lifecycle
STACK_SET_S3_EXPIRATION_DAYS=366
STACK_SET_S3_EXPIRED_DELETE_DAYS=30

# Deployment Targets
# OU ベースのデプロイ用: STACK_SET_TARGET_OUS を指定（SERVICE_MANAGED 推奨）
# アカウントベースのデプロイ用: STACK_SET_TARGET_ACCOUNTS を指定（SELF_MANAGED 推奨）
STACK_SET_TARGET_ACCOUNTS=210987654321,321098765432
# STACK_SET_TARGET_OUS=ou-xxxx-12345678,ou-yyyy-87654321
STACK_SET_TARGET_REGIONS=ap-northeast-1,us-east-1,eu-west-1
