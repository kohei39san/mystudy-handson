{
  "servers": {
    "awslabs.aws-documentation-mcp-server": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "tool",
        "run",
        "--from",
        "awslabs.aws-documentation-mcp-server@latest",
        "awslabs.aws-documentation-mcp-server.exe"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_DOCUMENTATION_PARTITION": "aws"
      }
    },
    "redmine-mcp-server": {
      "type": "stdio",
      "command": "npm",
      "args": [
        "start"
      ],
      "cwd": "030.redmine-mcp-server",
      "env": {
        "REDMINE_BASE_URL_PARAMETER": "/redmine/base-url",
        "AWS_REGION": "ap-northeast-1",
        "REDMINE_API_KEY_PARAMETER": "/redmine/api-key"
      }
    }
  }
}